#include "iostream"
#include "string"
#include "algorithm"
using namespace std;
int weight[5005]={0},value[5005]={0};
int dp[5005][5005]={0};
int main() {
    int n,bagsize;
    cin>>n>>bagsize;
    for (int i = 0; i < n; ++i) {
        cin>>weight[i];
    }for (int i = 0; i < n; ++i) {
        cin>>value[i];
    }
    for (int i = weight[0]; i <= bagsize; ++i) {
        dp[0][i]=value[0];
    }

    for (int i = 1; i < n; ++i) {//物品从1开始，初始化时已经将0物品写过了，？但为什么不能从0开始呢？
        for (int j = 0; j <= bagsize; ++j) {
            if(j<weight[i])dp[i][j]=dp[i-1][j];//这里要i-1所以不能从0开始，不然会溢出
            else dp[i][j]=max(dp[i-1][j],dp[i-1][j-weight[i]]+value[i]);
        }
    }
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j <= bagsize; ++j) {
            cout<<dp[i][j]<<' ';
        }
        cout<<endl;
    }
    cout<<dp[n-1][bagsize];
    return 0;
}
